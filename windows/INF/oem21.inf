[mvusbews.Dev.NT]
CopyFiles=mvusbews.Files.Ext


[mvusbews.Dev.NT.Services]
Addservice = mvusbews, %SPSVCINST_ASSOCSERVICE%, mvusbews.AddService

[mvusbews.AddService]
DisplayName    = %mvusbews.SvcDesc%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %10%\System32\Drivers\mvusbews.sys
LoadOrderGroup = Base

[mvusbews.Files.Ext]
mvusbews.sys

[SourceDisksNames.x86]
1=%Disk_Description%,,,x32

[SourceDisksNames.amd64]
1=%Disk_Description%,,,x64

[SourceDisksFiles]
mvusbews.sys = 1

[DestinationDirs]
DefaultDestDir = 12

;-------------- WDF Coinstaller installation

[DestinationDirs]
CoInstaller_CopyFiles = 11

[mvusbews.Dev.NT.CoInstallers]
AddReg=CoInstaller_AddReg
CopyFiles=CoInstaller_CopyFiles

[CoInstaller_CopyFiles]
WdfCoInstaller01007.dll
mvusbews.dll

[SourceDisksFiles]
WdfCoInstaller01007.dll=1 ; make sure the number matches with SourceDisksNames
mvusbews.dll=1

[CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller01007.dll,WdfCoInstaller"

[mvusbews.Dev.NT.Wdf]
KmdfService = mvusbews, mvusbews_wdfsect
[mvusbews_wdfsect]
KmdfLibraryVersion = 1.7

